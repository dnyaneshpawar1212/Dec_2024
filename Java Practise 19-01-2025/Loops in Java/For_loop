package ParcticeDemo;
public class All_Java {
	
	public static void main(String[] args) {

//for loop for(i=0;i<=10;i++)
	//	i=0 (starting point)
	//	i<=10(upto)
	//	i++ (how much incre/decr)
		
		//print 1 to 10
		
		int i;
		
		for(i=0;i<=10;i+=2)
			System.out.println(i);
		
		//print 10 to 1
		int j;
		
		for(j=10;j>=0;j--)
			System.out.println(j);
		}
}

		
