package ParcticeDemo;

public class All_Java {

	public static void main(String[] args) {

//Assignment Operators  == += -= *= /+ %= 
// It is also samr as increment and decrement operator.

int a=10,b=20;

		System.out.println("Equal to:" +(a=a) );
		System.out.println("Add equal: " + (a+=2));		
		System.out.println("sub equal: " + (b-=3));
		System.out.println("multi equal: " + (a*=4));
		System.out.println("div equal: " + (a/=5));
		System.out.println("per equal: " + (a%=10));
  
      }

}
